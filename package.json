    "@types/leaflet": "^1.7.0",
    "@types/node": "^15.0.1",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.46.0",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-terser": "^7.0.2",
